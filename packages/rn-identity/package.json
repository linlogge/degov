{
  "name": "@dgv/react-native-identity",
  "version": "0.1.0",
  "description": "S",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "react-native": "./lib/module/index.web.js",
      "browser": "./lib/module/index.web.js",
      "default": "./lib/module/index.web.js",
      "types": "./lib/typescript/src/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*",
    "!**/*.wasm.d.ts"
  ],
  "scripts": {
    "build:ios": "ubrn build ios --and-generate --release",
    "build:android": "ubrn build android --and-generate",
    "build:web": "ubrn build web",
    "build:bob": "bob build",
    "help": "ubrn --help",
    "clean": "del-cli -v cpp android/src/main/java ios \"src/Native*\" src/generated \"src/index.ts*\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linlogge/degov"
  },
  "author": "NoÃ«l Sigmunczyk <65312133+linlogge@users.noreply.github.com> (https://github.com/linlogge)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linlogge/degov/issues"
  },
  "homepage": "https://github.com/linlogge/degov",
  "dependencies": {
    "uniffi-bindgen-react-native": "github:jhugman/uniffi-bindgen-react-native#b930179"
  },
  "devDependencies": {
    "@react-native-community/cli": "20.0.1",
    "@react-native/babel-preset": "0.81.1",
    "@types/react": "^19.1.0",
    "del-cli": "^6.0.0",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.14",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "exclude": "src/generated/wasm-bindgen",
    "targets": [
      [
        "module",
        {
          "esm": true,
          "configFile": "./.babelrc"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "IdentitySpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "app.degov.identity"
    }
  },
  "create-react-native-library": {
    "languages": "kotlin-objc",
    "type": "turbo-module",
    "version": "0.54.8"
  }
}